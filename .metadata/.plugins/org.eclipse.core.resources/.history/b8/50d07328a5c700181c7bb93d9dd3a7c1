package testing;
import java.util.*;  
import java.io.*;

public class employee_Comparator 
{

	//Java Program to demonstrate the use of Java Comparable.  
	//Creating a class which implements Comparable Interface  
	  
	class Employee implements Comparable<Employee>
	{  
		int empID;  
		String empName;  
		int empAge; 
		String empContactNo;
		String empDesignation;
		
		Employee(int empID, String empName, int empAge, String empContactNo, String empDesignation)
		{  
			this.empID=empID;
			this.empName=empName;
			this.empAge=empAge;
			this.empContactNo=empContactNo;
			this.empDesignation=empDesignation;			  
		}
		
		//To compare employee 
		public int compareTo(Employee em)
		{  
			if(empAge==em.empAge)  
			return 0;  
			else if(empAge>em.empAge) 
			return 1;  
			else  
			return -1;  
		}  
	}  
	
	

	
	//Creating a test class to sort the elements  
	/*class TestSort3
	{  
		public static void main(String args[])
		{  
			ArrayList<Student> al=new ArrayList<Student>();  
			al.add(new Student(101,"Vijay",23));  
			al.add(new Student(106,"Ajay",27));  
			al.add(new Student(105,"Jai",21));  
			  
			Collections.sort(al);  
			for(Student st:al)
			{  
				System.out.println(st.rollno+" "+st.name+" "+st.age);  
			}  
		}  
	}  
*/
	
	public static void main(String args[])
	{ 
		employee_Comparator obj = new employee_Comparator();
	
		ArrayList<Employee> al=new ArrayList<Employee>();  
		al.add(obj.new Employee(100101,"Arjun",49,"8201876301","Software Engineer"));
		al.add(obj.new Employee(100102,"Arayan",24,"8201876302","Test Engineer"));
		al.add(obj.new Employee(100103,"Inisha",21,"8201876303","Developer"));
		al.add(obj.new Employee(100104,"Swapnil",86,"8201876304","Software Engineer"));
		al.add(obj.new Employee(100105,"Vivek",22,"8201876305","UFT"));
		al.add(obj.new Employee(100106,"Verra",18,"8201876306","Mainframe"));
		al.add(obj.new Employee(100107,"Dom",44,"8201876307","Test Lead"));
		
		  
		Collections.sort(al);  
		for(Employee em:al)
		{  
			System.out.println(em.empID+", "+em.empName+", "+em.empAge+", "+em.empContactNo+", "+em.empDesignation);			
		}		
		
	}
}

